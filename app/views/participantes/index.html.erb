<p id="notice"><%= notice %></p>

<h1>Participantes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @participantes.each do |participante| %>
      <tr>
        <td><%= link_to 'Exibir', participante %></td>
        <td><%= link_to 'Editar', edit_participante_path(participante) %></td>
        <td><%= link_to 'Excluir', participante, method: :delete, data: { confirm: 'Tem certeza?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Participante', new_participante_path %>
